{
    "manifest_version": 3,
    "version": "1.0.1",
    "name": "Baby's first extension",
    "content_scripts":[
      {
       "matches":["<all_urls>"],
       "js":["script.js"],
       "css": ["artreaderstyle.css"]
      }
    ],
    "web_accessible_resources": [
        {
            "resources": [ "/imgs/*.png" ],
            "matches": [ "<all_urls>" ]
        }
    ],
    "action": {
      "default_icon": "babysfirsticon.png",
      "default_popup": "index.html"
    }
  }